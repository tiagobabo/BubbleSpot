<section>
		 <div class="row">
      <div class="twelve columns"> 
        <div class="underline"><span class="text"><h1>Log in</h1></span></div> 
            
		 	</div>
		 </div>
     <p></p>
    <div class="row">
<div class="twelve columns">
<%= form_tag sessions_path do %>

    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email], :class => "medium input-text" %>
    
    <%= label_tag :password %>
    <%= password_field_tag :password, :class => "medium input-text" %>
  <p></p>
  <%= submit_tag "Log in", :class => "nice small radius blue button" %>
<% end %>
</div>
</div>
</section>








